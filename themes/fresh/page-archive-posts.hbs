{{!< default}}

{{! The main content area on the homepage }}
<main class="content module" id="content" role="main">
    <h1 class="tagPageHeading">Archive</h1>
    <hr>
    {{! Each post will be output using this markup }}
    <h1>All Posts</h1>
    {{#foreach posts}}
      {{#if @first}}
        {{else}}
        <hr>
      {{/if}}
      <article class="{{post_class}}">
        <h2 class="post-title"><a href="{{url}}">{{{title}}}</a></h2>
      </article>
    {{/foreach}}
</main>